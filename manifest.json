{
    "manifest_version": 2,

    "name": "Sleep Countdown",
    "description": "Tells you how much sleep you'll get.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}